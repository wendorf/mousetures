{
  "manifest_version": 2,

  "name": "Mousetures",
  "description": "Mouse and rocker gestures for tab switching, closing, and reloading",
  "version": "0.5",

  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["mouseStatus.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["mouseEventGroup.js", "mouseStatus.js", "point.js", "moveGestures.js", "main.js"]
    }
  ]
}